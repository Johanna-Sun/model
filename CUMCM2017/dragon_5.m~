

clear all;
clc;

b = 1;
hold on
i = 1;
while i < 4
t = (5/12)*(3/2).^i*(-(1/3)*(-1).^i - (10/3)):0.0001:(5/12)*(3/2).^i*(-(1/3)*(-1).^i + (10/3));
x = t;
y = sqrt(-(24/25)*(t.^2) - (4/15)*(-(3/2);
z = -sqrt((1/4)*(t.^2) - ((-1).^i)*(3.^i)*t*b/3 + (((3.^i)*b)^2)/9);
plot3(x,y,z);
i = i + 1;
t = 2*3.^i/9*(-(-1).^i-2):0.0001:2*3.^i/9*(-(-1).^i+2);
x = t;
y = -sqrt(-(3/4)*(t.^2) - ((-1).^i)*(3.^i)*t*b/3 + (((3.^i)*b)^2)/9);
z = -sqrt((1/4)*(t.^2) - ((-1).^i)*(3.^i)*t*b/3 + (((3.^i)*b)^2)/9);
i = i + 1;
plot3(x,y,z);
grid on;
end
hold off